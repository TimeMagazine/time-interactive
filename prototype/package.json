{
  "name": "<%= interactive_id %>",
  "version": "0.0.0",
  "description": "",
  "repository": {},
  "dependencies": {
    "time-interactive": "0.5.5"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack --config webpack/development.config.js --mode development",
    "minify": "./node_modules/.bin/webpack --config webpack/production.config.js --mode production",
    "watch": "./node_modules/.bin/webpack-dev-server --config webpack/development.config.js --mode development --watch",
    "analyze": "./node_modules/.bin/webpack --config webpack/production.config.js --json | webpack-bundle-size-analyzer",
    "dryrun": "aws s3 sync . s3://time-static-shared/interactives/$npm_package_name/ --exclude '*code/*' --exclude '*node_modules/*' --exclude='*git/*' --exclude='*DS_Store*' --dryrun",
    "upload": "aws s3 sync . s3://time-static-shared/interactives/$npm_package_name/ --exclude '*code/*' --exclude '*node_modules/*' --exclude='*git/*' --exclude='*DS_Store*'",
    "minimal": "npm run build && npm run minify && aws s3 sync . s3://time-static-shared/interactives/$npm_package_name/ --exclude '*' --include='script.js --include='script-min.js' --include='index.html'",
    "stage": "npm run build && npm run minify && aws s3 sync . s3://time-static-shared/interactives/staging/$npm_package_name/ --exclude '*node_modules/*' --exclude='*git/*' --exclude='*DS_Store/*'",
    "deploy": "npm run build && npm run minify && npm run upload",
    "url": "echo http://time-static-shared.s3-website-us-east-1.amazonaws.com/interactives/$npm_package_name/",
    "staging_url": "echo http://time-static-shared.s3-website-us-east-1.amazonaws.com/interactives/staging/$npm_package_name/"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-loader": "1.0.0",
    "dsv-loader": "2.0.0",
    "ejs-loader": "^0.3.1",
    "file-loader": "^2.0.0",
    "fs-extra": "7.0.0",
    "less": "^3.0.4",
    "less-loader": "4.1.0",
    "node-sass": "4.9.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.22.1",
    "url-loader": "^1.0.1",
    "webpack": "4.17.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "3.1.5",
    "webpack-merge": "^4.1.2"
  }
}
